[color]
  ui = true
[core]
  excludesfile = ~/.gitignore.global
  precomposeUnicode = true
[apply]
  whitespace = nowarn
[credential]
  helper = osxkeychain
[push]
  default = current
[mergetool]
	keepBackup = false
	keepTemporaries = false
[merge]
  tool = vimdiff
[include]
  path = .gitconfig.local
[branch]
	sort = -committerdate
[alias]
	root = rev-parse --show-toplevel
	exec = "!exec "
	wtstat = each-worktree --header status --short --branch
