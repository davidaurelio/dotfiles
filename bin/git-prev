#!/bin/bash

set -eu -o pipefail

PARENTS=($(git rev-parse HEAD^@))

case ${#PARENTS[@]}
in
  0)
    echo "Commit has no parents" >&2
    exit 2;;
  1)
    git checkout "$(git-canonical ${PARENTS[0]})";;
  *)
    echo "Commit has multiple parents: " >&2
    echo "${PARENTS[@]}" >&2
    exit 1;;
esac
